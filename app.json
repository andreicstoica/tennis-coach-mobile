{
  "expo": {
    "name": "tennis-coach-mobile",
    "slug": "tennis-coach-mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "tenniscoachmobile",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "fonts": [
      {
        "asset": "./assets/fonts/IBMPlexSans-Regular.ttf",
        "family": "IBMPlexSans"
      },
      {
        "asset": "./assets/fonts/IBMPlexSans-Medium.ttf",
        "family": "IBMPlexSans-Medium"
      },
      {
        "asset": "./assets/fonts/IBMPlexSans-SemiBold.ttf",
        "family": "IBMPlexSans-SemiBold"
      },
      {
        "asset": "./assets/fonts/IBMPlexSans-Bold.ttf",
        "family": "IBMPlexSans-Bold"
      }
    ]
  }
}
