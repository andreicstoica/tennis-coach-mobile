{
  "cli": {
    "version": ">= 16.13.2",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },
    "preview": {
      "distribution": "internal"
    },
    "production": {
      "autoIncrement": true
    },
    "ios": {
      "developmentClient": false,
      "distribution": "internal",
      "cocoapods": "1.15.2",
      "env": {
        "EXPO_USE_UNIMODULES": "1"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
